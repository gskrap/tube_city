<div class="jumbotron">
  <h1>Sign In</h1>
  <%= form_tag sessions_path, id: "new-session-form" %>
    <%= render 'errors' %>
    <%= text_field_tag :username, nil, placeholder: "Username" %><br />
    <%= password_field_tag :password, nil, placeholder: "Password" %><br />
    <%= submit_tag "Log In", class: 'btn btn-primary'%>
</div>